<?xml version="1.0" encoding="utf-8"?>
<shape android:shape="rectangle" xmlns:android="http://schemas.android.com/apk/res/android">

    <!--Radius of all four corners-->
    <corners
        android:bottomRightRadius="20dp"
        android:topRightRadius="20dp"/>

    <solid android:color="@color/green_one"/>

    <!--Stroke size and color-->
    <stroke android:width="2dp"
        android:color="@color/green_one" />



    <!--Fill Color of the Button-->
    <gradient android:angle="-90"
        android:startColor="@color/green_one"
        android:endColor="@color/green_one"  />

</shape>